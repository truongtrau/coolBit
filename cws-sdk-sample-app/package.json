    "lint": "eslint .",
    "prepare": "npm run patch:replace_android_support_v4 && npm run patch:replace_android_support && npm run patch:npx",
    "patch:replace_android_support_v4": "find ./node_modules/* -name '*.java' -print -exec sed -i '' 's/android\\.support\\.v4/androidx\\.core/g' {} \\;",
    "patch:replace_android_support": "find ./node_modules/* -name '*.java' -print -exec sed -i '' 's/android\\.support/androidx/g' {} \\;",
    "patch:npx": "npx rn-nodeify --install --hack",
    "run-android": "react-native run-android",
    "log-android": "react-native log-android"
  },
